<div class="row">
    <div class="col-md-12">
  
      <% if (typeof request.status !== "undefined") { %>
        <% if (request.status == "success") { %>
          <script>
            window.alert("<%= request.message %>");
          </script>
        <% } else if (request.status == "error") { %>
          <script>
            window.alert("<%= request.message %>");
          </script>
        <% } %>
      <% } %>
  
      <% if (typeof request.session.status !== "undefined") { %>
        <% if (request.session.status == "success") { %>
          <script>
            window.alert("<%= request.session.message %>");
          </script>
        <% } else if (request.session.status == "error") { %>
          <script>
            window.alert("<%= request.session.message %>");
          </script>
        <% } %>
        delete request.session.status;
        delete request.session.message;
      <% } %>
  
    </div>
  </div>
  